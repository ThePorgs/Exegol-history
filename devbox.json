{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "poetry@latest",
    "python@3.13",
    "python3Packages.rich",
    "ruff@latest"
  ],
  "shell": {
    "init_hook": [
      "poetry install"
    ],
    "scripts": {
      "exegol-history": "poetry run python exegol-history.py",
      "test": "poetry run pytest -v",
      "format": "ruff format ."
    }
  }
}
